{
    "name": "Raphire's Curation",
    "version": "1.0.0",
    "description": "Saves Twitter Favs to DB",
    "manifest_version": 3,
    "author": "Raphire",
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["https://twitter.com/*"]
        }
    ]
}